{
  "require": [
    "./nyc-babel-register.cjs"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.js"
  ],
  "exclude": [
    "test/**/*.ts",
    "test/**/*.js"
  ],
  "extension": [
    ".ts",
    ".js"
  ],
  "reporter": [
    "text",
    "text-summary"
  ],
  "all": true,
  "cache": false,
  "sourceMap": false
}
